# Turso Database Configuration
# Get your HTTP URL using: turso db show <database-name> --http-url
# The URL should be in format: libsql://xxx-xxx.turso.io
EXPO_PUBLIC_TURSO_URL=libsql://your-database-name-your-org.turso.io

# Create auth token using: turso db tokens create <database-name>
EXPO_PUBLIC_TURSO_AUTH_TOKEN=your-turso-auth-token
